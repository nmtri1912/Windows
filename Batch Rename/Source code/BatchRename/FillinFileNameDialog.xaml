<Window x:Class="BatchRename.FillinFileNameDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        WindowStartupLocation="CenterScreen"
        xmlns:local="clr-namespace:BatchRename"
        mc:Ignorable="d"
        Title="FillinFileNameDialog" Height="180" Width="330">
    <Grid>
        <Label HorizontalAlignment="Left" Margin="90,10,0,0">Input preset name to save</Label>
        <TextBox x:Name="fileName" HorizontalAlignment="Left" Height="23" Margin="94,36,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="saveFileName" Content="Save" Click="btnSave_Click" HorizontalAlignment="Left" Margin="49,95,0,0" VerticalAlignment="Top" Width="75"/>
        <Button x:Name="cancel" Content="Cancel" IsCancel="True" HorizontalAlignment="Left" Margin="181,95,0,0" VerticalAlignment="Top" Width="75"/>

    </Grid>
</Window>
